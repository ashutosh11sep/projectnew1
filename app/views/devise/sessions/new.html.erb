
<body style="padding-top:150px;">
<div class="container">
<center>
<div class="container">
          <h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field", class="eml", placeholder="Email",  >
    <%#= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, placeholder: "Email", id: "email" %>
  </div>

  <div class="field", class="eml" >
    <%#= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password", id: "pass" %>
  </div>

  <%# if devise_mapping.rememberable? -%>
    <!-- <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>  -->

    <small>Need to Coleb? <%= link_to "Sign up", new_registration_path(resource_name) %><br /></a></small>
    <h3>OR</h3>     
  <%# end -%>

  



<% unless current_user %>
 <div class="btn-group-vertical" >
 <button style="padding:5px 60px;" type="button" class="btn btn-info face">
  <%= link_to "Facebook", user_facebook_omniauth_authorize_path %></button>
  <br/>
 <div class="actions", class="btn-group-vertical">
    <button style="padding:5px 60px;" type="button">
    <%= f.submit "Log in" %></br></button></div>
<%# else %>
  <%#= link_to "Logout", destroy_user_session_path, method: :delete %>
 
 <% end %>
 </div>
<% end %>
</center>
</div>
</body>


<%#= render "devise/shared/links" %>






